# Generate noise
noise <-  rnorm(10000)
ma_2 <- NULL
for(i in 3:10000){
ma_2[i] <- noise[i] + 0.7* noise[i-1] + 0.2 * noise[i-2]
}
moving_average_process <- ma_2[3:10000]
par(mfrow = c(2,1))
plot(ts(moving_average_process))
acf(moving_average_process)
